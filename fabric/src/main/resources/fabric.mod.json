{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "${sources_url}",
    "issues": "${issues_url}"
  },
  "license": "${license}",
  "icon": "${mod_id}_icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.illusivesoulworks.comforts.data.ComfortsDataGenerator"
    ],
    "main": [
      "com.illusivesoulworks.comforts.ComfortsFabricMod"
    ],
    "client": [
      "com.illusivesoulworks.comforts.ComfortsFabricClientMod"
    ],
    "spectrelib-config": [
      "com.illusivesoulworks.comforts.ComfortsConfigInitializer"
    ],
    "cardinal-components-entity": [
      "com.illusivesoulworks.comforts.common.ComfortsComponents"
    ]
  },
  "custom": {
    "cardinal-components": [
      "comforts:sleep_tracker"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "fabric-api": "*",
    "minecraft": "${minecraft_version_range_alt}",
    "java": ">=${java_version}"
  }
}
  